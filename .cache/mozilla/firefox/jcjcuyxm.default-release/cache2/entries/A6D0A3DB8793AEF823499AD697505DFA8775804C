<!-- Leaflet JS -->
<script src="https://unpkg.com/leaflet/dist/leaflet.js"></script>

<script>
    // Initial map position
    let map = L.map('map').setView([10.0, 78.0], 12);

    // Add OpenStreetMap tiles
    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
        maxZoom: 19,
        attribution: '¬© OpenStreetMap'
    }).addTo(map);

    // Add marker
    let marker = L.marker([10.0, 78.0]).addTo(map);
    
    // Add circle for detection area
    let detectionRadius = L.circle([10.0, 78.0], {
        color: 'red',
        fillColor: '#f03',
        fillOpacity: 0.1,
        radius: 500
    }).addTo(map);

    // Detection history array
    let detectionHistory = [];
    let detectionCount = 0;

    // Demo Data Updater
    function updateDemoData() {
        const lat = (10 + Math.random() * 0.1).toFixed(6);
        const lng = (78 + Math.random() * 0.1).toFixed(6);
        const humanDetected = Math.random() > 0.7;

        document.getElementById("lat").innerText = lat;
        document.getElementById("lng").innerText = lng;

        const alertBox = document.getElementById("alertBox");

        if (humanDetected) {
            alertBox.className = "alert alert-danger";
            alertBox.innerHTML = '<i class="fas fa-exclamation-triangle"></i> Human Detected!';
            
            // Add to detection history
            detectionCount++;
            document.getElementById("detectionCount").innerText = detectionCount;
            
            const now = new Date();
            const timeString = now.toLocaleTimeString();
            detectionHistory.unshift({
                time: timeString,
                location: `${lat}, ${lng}`
            });
            
            // Keep only last 5 entries
            if (detectionHistory.length > 5) {
                detectionHistory.pop();
            }
            
            updateHistoryList();
        } else {
            alertBox.className = "alert alert-safe";
            alertBox.innerHTML = '<i class="fas fa-check-circle"></i> No Human Detected';
        }

        // Update marker position on map
        marker.setLatLng([lat, lng]);
        detectionRadius.setLatLng([lat, lng]);
        map.setView([lat, lng]);
    }

    function updateHistoryList() {
        const historyList = document.getElementById("historyList");
        historyList.innerHTML = '';
        
        if (detectionHistory.length === 0) {
            historyList.innerHTML = '<div class="history-item" style="padding: 10px; border-bottom: 1px solid #eee; display: flex; justify-content: space-between;"><span>No recent detections</span><span style="color: var(--gray);">--</span></div>';
        } else {
            detectionHistory.forEach(item => {
                const historyItem = document.createElement('div');
                historyItem.className = 'history-item';
                historyItem.style.cssText = 'padding: 10px; border-bottom: 1px solid #eee; display: flex; justify-content: space-between;';
                historyItem.innerHTML = `<span>Detection at ${item.location}</span><span style="color: var(--gray);">${item.time}</span>`;
                historyList.appendChild(historyItem);
            });
        }
    }

    // Initialize
    setInterval(updateDemoData, 5000);
    updateDemoData();
    updateHistoryList();
</script>

